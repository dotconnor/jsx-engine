{
  "name": "jsx-engine",
  "description": "JSX renderer for Express",
  "version": "1.3.2",
  "main": "lib/index.js",
  "license": "MIT",
  "author": {
    "email": "connor@dotconnor.com",
    "name": "Connor Love"
  },
  "keywords": [
    "jsx",
    "express",
    "template",
    "engine"
  ],
  "files": [
    "lib/*",
    "readme.md",
    "LICENSE"
  ],
  "types": "lib/",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --format=\"pretty\" './src/**/*.ts'",
    "test": "nyc --reporter=lcov --reporter=text ava --verbose"
  },
  "homepage": "https://github.com/dotconnor/jsx-engine",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotconnor/jsx-engine.git"
  },
  "dependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/plugin-transform-react-jsx": "^7.7.4",
    "add-px-to-style": "^1.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/babel__core": "^7.1.3",
    "@types/node": "^13.9.0",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "ava": "^2.4.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-config-a1motion": "^3.1.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "express": "^4.17.1",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "./__tests__/_register.js"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true
  }
}
